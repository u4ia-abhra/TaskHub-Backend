{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "crons": [
    {
      "path": "/api/cron/autoPayout",
      "schedule": "0 * * * *"
    }
  ],
  "functions": {
    "api/cron/autoPayout.js": {
      "runtime": "nodejs18.x",
      "maxDuration": 60
    }
  }
}
